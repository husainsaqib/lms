<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/all.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/2.1.7/css/dataTables.dataTables.min.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="https://cdn.datatables.net/2.1.7/js/dataTables.min.js"></script>
    <script src="~/Content/js/bootstrap.bundle.js"></script>
    <style>
        :root {
            --color1: #1cb69a;
            --color2: #ee4a62;
            --color3: #1f2432;
        }

        .bg-mycolor1 {
            background: var(--color1);
        }

        .bg-mycolor2 {
            background: var(--color2);
        }

        .bg-mycolor3 {
            background: var(--color3);
        }

        .txt-color1 {
            color: var(--color1) !important;
        }


        .txt-color2 {
            color: var(--color2) !important;
        }

        .txt-color3 {
            color: var(--color3) !important;
        }

        .ic {
            border: 1px solid white;
            height: 20px;
            width: 20px;
            line-height: 20px;
            transition: all .6s;
        }

            .ic:hover {
                border: 1px solid var(--color1);
                color: var(--color1);
                transition: all .6s;
                transform: rotate(360deg);
            }

        .t1 {
            font-size: 14px;
        }

            .t1 a {
                text-decoration: none;
                color: white;
            }

        #menu {
            background: transparent !important;
        }

        .slider {
            min-height: 450px;
            background: url('../Content/images/s1.jpg');
            background-attachment: fixed;
            background-size: cover;
        }

        .login {
            background: rgba(255,255,255,.2);
            margin-top: 10%;
            padding: 4%;
            backdrop-filter: blur(5px);
            color: white;
        }

        .form-control {
            border-radius: 0px !important;
            height: 50px;
            margin-bottom: 10px;
        }

        .btn1 {
            height: 50px;
            width: 120px;
            border: none;
            color: white;
        }

        .sicon {
            height: 90px;
            width: 90px;
            background: var(--color2);
            text-align: center;
            line-height: 90px;
            border-radius: 50%;
            font-size: 40px;
            color: white;
        }
    </style>
</head>
<body>
    <div class="container-fluid">
        <div class="row t1 bg-mycolor3 text-light text-center py-1">
            <div class="col-sm-4">

                <a href="#">Contact & Support</a> |
                <a href="#" class="txt-color2">Sign In</a> |
                <a href="#" class="txt-color2">Change Password</a>
            </div>
            <div class="col-sm-4">
                Welcome to My Learning Management Software
            </div>
            <div class="col-sm-4">
                <i class="fa-brands fa-facebook ic"></i>
                <i class="fa-brands fa-twitter ic"></i>
                <i class="fa-brands fa-youtube ic"></i>
                <i class="fa-brands fa-instagram ic"></i>
                <i class="fa-brands fa-linkedin ic"></i>


            </div>
        </div>

        <div class="row t2 py-4 shadow bg-body-tertiary rounded">
            <div class="col-sm-1"></div>
            <div class="col-sm-10">
                <div class="row">
                    <div class="col-sm-2 fs-4 pt-1">
                        EDU <b class="txt-color1">
                            Learning
                            <i class="fa-solid fa-graduation-cap"></i>
                        </b>
                    </div>
                    <div class="col-sm-4">

                       

                    </div>

                    <div class="col-sm-2">
                        <i class="fa-solid fa-phone txt-color2"></i>
                        <b>Call Us</b><br />
                        +91 7084059131

                    </div>
                    @if (Session["admin"]==null)
            {
                <script>
                    alert('Please Login');
                    location.href = '/home/adminlogin';
                </script>
            }
                    <div class="col-sm-2"><a href="/admin/logout" class="btn my-1">Log out</a></div>
                </div>
            </div>
           
        </div>
        <!--start of menu section-->
        <div class="row t2 py-4 shadow bg-body-tertiary rounded">
            <div class="col-sm-1"></div>
            <div class="col-sm-10">
                <nav class="navbar navbar-expand-lg bg-body-tertiary p-0" id="menu">
                    <div class="container-fluid">

                        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>
                        <div class="collapse navbar-collapse" id="navbarNav">
                            <ul class="navbar-nav">
                                <li class="nav-item">
                                    <a class="nav-link active" aria-current="page" href="/student/dashboard"><i class="fa-solid fa-house"></i>Dashboard</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/Admin/AddBatch">Manage Batches</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/Admin/AddVideoCategory">Manage Video Category</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/Admin/SubmitVideo">Manage Video </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/Admin/Assignment">Add Assignment</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/admin/manageassignment">Manage Assignment</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="admin/manageenquiry">Manage Enquiry</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/admin/managestudent">Manage Student</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/admin/notes">Manage Notes</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="#">Logout</a>
                                </li>



                            </ul>
                        </div>
                    </div>
                </nav>

            </div>
            <div class="col-sm-1"></div>
        </div>


        <!--end of menu section-->
        <div class="row childpage" style="min-height:500px;">
            <div class="col-sm-12">
                @RenderBody()
            </div>
        </div>
       
        <div class="row bfooter bg-mycolor3 text-center py-2 text-light">
            <div class="row bfooter bg-mycolor3 text-light text-center py-2">
                <div class="col-sm-6">
                    Developed By : Er Mohd Saqib
                </div>
                <div class="col-sm-6">
                    &copy; Techpile Technology Pvt. Ltd.
                </div>
            </div>

    </div>
</div>
</body>

</html>
